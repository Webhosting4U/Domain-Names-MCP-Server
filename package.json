  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "agents": "^0.5.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.14",
    "@cloudflare/workers-types": "^4.20250219.0",
    "tsx": "^4.21.0",
